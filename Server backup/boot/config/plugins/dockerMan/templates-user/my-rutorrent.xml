<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>rutorrent</Name>
  <Description>Popular torrent client with a webui for ease of use. &#13;
[b][u][span style='color: #E80000;']Configuration[/span][/u][/b][br]&#13;
[b]/config[/b] Where rutorrent should store configuration files[br]&#13;
[b]/downloads[/b] Downloads folder[br]&#13;
</Description>
  <Registry>https://hub.docker.com/r/linuxserver/rutorrent/</Registry>
  <Repository>linuxserver/rutorrent</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8080</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>45566</HostPort>
        <ContainerPort>45566</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>45567</HostPort>
        <ContainerPort>45567</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>45568</HostPort>
        <ContainerPort>45568</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>45569</HostPort>
        <ContainerPort>45569</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>45570</HostPort>
        <ContainerPort>45570</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>45571</HostPort>
        <ContainerPort>45571</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>45572</HostPort>
        <ContainerPort>45572</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>45573</HostPort>
        <ContainerPort>45573</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>45574</HostPort>
        <ContainerPort>45574</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>45575</HostPort>
        <ContainerPort>45575</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>45576</HostPort>
        <ContainerPort>45576</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>9527</HostPort>
        <ContainerPort>9527</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/rutorrent/</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/Media/automation/downloading/</HostDir>
      <ContainerDir>/downloads</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/Media/Movies/</HostDir>
      <ContainerDir>/movies</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/Media/TV/</HostDir>
      <ContainerDir>/tv</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/Media/automation/movies/</HostDir>
      <ContainerDir>/automated</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/Media/Music/</HostDir>
      <ContainerDir>/music</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/Media/Books/</HostDir>
      <ContainerDir>/books</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI>http://[IP]:[PORT:80]/</WebUI>
  <Banner></Banner>
  <Icon>https://raw.githubusercontent.com/linuxserver/beta-templates/master/lsiodev/img/rutorrent-icon.png</Icon>
  <ExtraParams></ExtraParams>
</Container>
